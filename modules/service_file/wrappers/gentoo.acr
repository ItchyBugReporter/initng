#!/bin/sh
export PATH="@LIBDIR@/ibin:$PATH"

. /sbin/functions.sh
[ -f "/etc/conf.d/$NAME" ] && . "/etc/conf.d/$NAME"
. "$SFILE"

setup() {
	iregister service || exit
	iexec start
	for i in stop $opts; do
		if typeset -F \"$i\" &>/dev/null; then
			iexec $i
			echo $i
		fi
	done

	need() { iset need = "$@"; }
	use() { iset use = "$@"; }
	depend
	idone
}

$1
