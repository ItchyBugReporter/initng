#!/bin/sh

eval "
${CMD}()
{
	echo \"${SERVICE}: command ${CMD} not found.\"
	return 1
}
"

ireg()
{
	[ -n "${2}" ] && export SERVICE="${2}"
	iregister "${1}" || exit ${?}
}

is_service()
{
	while [ ${#} -gt 0 ]; do
		[ "${SERVICE}" = "${1}" ] && return 0
		shift
	done
	return 1
}

export PATH="@LIB_INSTALL_DIR@/ibin:${PATH}"
source "${SFILE}"

${CMD}
