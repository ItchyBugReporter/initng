#!/sbin/runiscript

setup()
{
    iregister service $SERVICE
    iset $SERVICE need = "checkroot mountroot"
	reg_start
	reg_stop
    idone $SERVICE
}

start()
{
	echo "Starting $SERVICE"
    echo "init3g v0.0.1" > /etc/issue
	echo "env:"
	/usr/bin/env
    exit 0
}

stop()
{
	echo "Stopping $SERVICE here"
    rm /etc/issue
    exit 0
}
